
public class Human {
	
	static int count_Human=0;
	static int count_arms =2;
	static int count_legs =2;
	
	String name;
	int birthday;
	int manpukudo;
	
	Human(String name,int birthday,int manpukudo){
		this.name=name;
		this.birthday=birthday;
		this.manpukudo=manpukudo;
		count_Human ++;
	}
	
	Human(String name,int birthday){
		this(name,birthday,50);
	}
	
	Human(String name){
		this(name,0,50);
	}
	
	Human(){
		this("不明",0,50);
	}
	
}
