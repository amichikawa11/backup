
public class Human {
	String name;
	int birthday;
	int manpukudo;
	
	Human(String name,int birthday,int manpukudo){
		this.name=name;
		this.birthday=birthday;
		this.manpukudo=50;	
	}
	

	
	Human(String name,int birthday){
		this(name,birthday,50);
	}
	
	Human(String name){
		this(name,0,50);
	}
	
	Human(){
		this("不明",0,50);
	}
	

	void eat(){
		this.manpukudo += 60;
	}

}
