package com.internousdev.myec.action;

import org.apache.struts2.interceptor.SessionAware;
import java.util.Map;
import com.opensymphony.xwork2.ActionSupport;

public class LoginMovementAction extends ActionSupport implements SessionAware{

	private String loginMessage;

	public Map<String, Object> session;

	public String execute(){

		if(session.containsKey("masterId")){

			loginMessage = "管理者IDでログイン済です。";


		}else if(session.containsKey("id")){

			loginMessage = "session.userName + 'でログイン済です。'";
		}

		return SUCCESS;

	}

	public String getLoginMessage() {
		return loginMessage;
	}

	public void setLoginMessage(String loginMessage) {
		this.loginMessage = loginMessage;
	}

	public Map<String, Object> getSession() {
		return session;
	}

	public void setSession(Map<String, Object> session) {
		this.session = session;
	}


}
