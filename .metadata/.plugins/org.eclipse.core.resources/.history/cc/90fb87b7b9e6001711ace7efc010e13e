public class Score{
     public static void main(String[] args){

    	powerOfTwo(5);
     }

     public static void powerOfTwo(int n){
    	 int answer = 1;

    	 for(int i=0; i<n; i++){
    		 answer = answer *2;
    	 }

    	 System.out.print("2び"+n+"乗は");
    	 System.out.println(answer);

     }
     }
